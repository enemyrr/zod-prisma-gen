{
  "name": "prisma-to-zod",
  "version": "0.1.0",
  "description": "A simple Prisma generator that creates Zod validation schemas",
  "main": "dist/generator.js",
  "bin": {
    "prisma-to-zod": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npx prisma generate --schema=./test/schema.prisma"
  },
  "keywords": [
    "prisma",
    "zod",
    "generator",
    "validation",
    "schema"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/generator-helper": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prisma": "^6.0.0",
    "typescript": "^5.0.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  }
}
